{{ form_start(form) }}
 {% set run = form.vars.value %}
<div class="modal-body">
                <div class="row">
                    <div class="form-group col-md-12">
                        {{ form_row(form.plot) }}
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        {{ form_row(form.pondingStart) }}
                    </div>
                    <div class="form-group col-md-6">
                        {{ form_row(form.runoffStart) }}
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        {{ form_row(form.cropBBCH) }}
                    </div>
                    <div class="form-group col-md-6">
                        {{ form_row(form.surfaceCover) }}
                    </div>
                </div>

                <div class="row">
                    <div class="form-group col-md-12">
                        <div class="nav-tabs-custom">
                            <ul class="nav nav-tabs">
                                <li class="active">
                                    <a href="#Run_{{ run.getId() }}_cropConditionCZ" data-toggle="tab" aria-expanded="true">{{ form_label(form.cropConditionCZ) }}</a>
                                </li>
                                <li class="">
                                    <a href="#Run_{{ run.getId() }}_cropConditionEN" data-toggle="tab" aria-expanded="false">{{ form_label(form.cropConditionEN) }}</a>
                                </li>
                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane active" id="Run_{{ run.getId() }}_cropConditionCZ">
                                    {{ form_widget(form.cropConditionCZ) }}
                                </div>
                                <div class="tab-pane" id="Run_{{ run.getId() }}_cropConditionEN">
                                    {{ form_widget(form.cropConditionEN) }}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-12">
                        <div class="nav-tabs-custom">
                            <ul class="nav nav-tabs">
                                <li class="active">
                                    <a href="#Run_{{ run.getId() }}_noteCZ" data-toggle="tab" aria-expanded="true">{{ form_label(form.noteCZ) }}</a>
                                </li>
                                <li class="">
                                    <a href="#Run_{{ run.getId() }}_noteEN" data-toggle="tab" aria-expanded="false">{{ form_label(form.noteEN) }}</a>
                                </li>
                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane active" id="Run_{{ run.getId() }}_noteCZ">
                                    {{ form_widget(form.noteCZ) }}
                                </div>
                                <div class="tab-pane" id="Run_{{ run.getId() }}_noteEN">
                                    {{ form_widget(form.noteEN) }}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-12">
                        {{ form_row(form.methodics) }}
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            {{ form_row(form.rawData) }}
                            <p class="help-block">Max. 32MB, *.zip</p>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        {{ form_row(form.soilSampleCorg) }}
                    </div>
                    <div class="form-group col-md-6">
                        {{ form_row(form.corgAssignmentType) }}
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        {{ form_row(form.soilSampleBulk) }}
                    </div>
                    <div class="form-group col-md-6">
                        {{ form_row(form.bulkAssignmentType) }}
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        {{ form_row(form.soilSampleTexture) }}
                    </div>
                    <div class="form-group col-md-6">
                        {{ form_row(form.textureAssignmentType) }}
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-12">
                        {{ form_row(form.referenceRun) }}
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-warning pull-left" data-dismiss="modal">
                    {% trans %}close{% endtrans %}
                </button>
                {{ form_row(form.save) }}
            </div>
{{ form_end(form, {'render_rest': false}) }}
